<div class="form-container">
<%= simple_form_for(event) do |f| %>
  <%= f.input :location %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :title %>
  <%= f.input :capacity %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file, input_html: { required:false }%>
  <%= f.submit %>
<% end %>
</div>
